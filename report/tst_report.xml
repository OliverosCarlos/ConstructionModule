<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
    <record id="action_project_report_tst" model="ir.actions.report">
       <field name="name">Reporte del proyecto</field>
       <field name="model">construction.project</field>
       <field name="report_type">qweb-pdf</field>
       <field name="report_name">construction.project_report_tst</field>
       <field name="report_file">construction.project_report_tst</field>
       <field name="print_report_name">'Proyecto - %s' % (object.name)</field>
       <field name="binding_model_id" ref="model_construction_project"/>
    </record>

    <template id="project_report_tst">
    <t t-call="web.html_container">
        <t t-foreach="docs" t-as="o">
            <t t-call="web.external_layout">
                <div class="page">
                    <h2>Report title</h2>
                    <p>This object's name is <span t-field="o.name"/></p>
                </div>
            </t>
        </t>
    </t>
</template>
    </data>
</odoo>